    "# 合并数据并计算房价租金比\n",
    "merged = pd.merge(\n",
    "    housing_results[['location', 'm2', 'total_value']],\n",
    "    renting_results[['location', 'm2', 'total_value']],\n",
    "    on=['location', 'm2'],\n",
    "    suffixes=('_price', '_rent')\n",
    ")\n",
    "\n",
    "merged['price_rent_ratio'] = merged['total_value_price'] / merged['total_value_rent']"
   ]
